#include <iostream>
#include <vector>
#include <cmath>

using namespace std;

int euclid(int n1, int n2)
{
    while(n1 != n2)
    {
        if(n1 > n2) n1 -= n2;
        else n2 -= n1;
    }
    return n1;
}

int sum_vec(vector<int> a, int (*ptrfun)(int,int))
{
    long long sum = 0;
    for(int i = 0; i < a.size(); ++i)
    {
        for(int j = i + 1; j < a.size(); ++j)
        {
           for(int k = i; k <= j; ++k)
           {
               sum += (ptrfun(a[i],a[k]) * ptrfun(a[k],a[j]));
           }
        }
    }
    return sum;
}

int main()
{
    vector<int> vec{2,4,8,16};            
    cout << "sum : " << sum_vec(vec,euclid);
    return 0;
}
